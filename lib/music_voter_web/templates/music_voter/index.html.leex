<div>
  <ul>
  <%= for song <- @songs do %>
    <li>
      <button phx-click="inc" phx-value=<%= song.id %>>+</button>
      <%= song.url %>
      <button phx-click="dec" phx-value=<%= song.id %>>-</button>
    </li>
  <% end %>
  </ul>
  <form action="#" phx-submit="add">
    <%= text_input :song, :url, placeholder: "Enter a youtube url" %>
    <%= submit "Add", phx_disable_with: "Adding" %>
  </form>
</div>
